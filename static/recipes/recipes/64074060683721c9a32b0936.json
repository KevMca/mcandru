rketplaceItems": [], "name": "Aubergine Saag\t\t\t", "nutrition": [{"type": "57b42a48b7e8697d4b305304", "name": "Energy (kcal)", "amount": 528, "unit": "kcal"}, {"type": "57b42a48b7e8697d4b30530d", "name": "Energy (kJ)", "amount": 2211, "unit": "kJ"}, {"type": "57b42a48b7e8697d4b305307", "name": "Fat", "amount": 18.2, "unit": "g"}, {"type": "57b42a48b7e8697d4b305308", "name": "of which saturates", "amount": 14.6, "unit": "g"}, {"type": "57b42a48b7e8697d4b305305", "name": "Carbohydrate", "amount": 83, "unit": "g"}, {"type": "57b42a48b7e8697d4b305306", "name": "of which sugars", "amount": 10, "unit": "g"}, {"type": "57b42a48b7e8697d4b30530a", "name": "Dietary Fiber", "amount": 0, "unit": "g"}, {"type": "57b42a48b7e8697d4b305309", "name": "Protein", "amount": 13, "unit": "g"}, {"type": "57b42a48b7e8697d4b30530c", "name": "Cholesterol", "amount": 0, "unit": "mg"}, {"type": "57b42a48b7e8697d4b30530b", "name": "Salt", "amount": 0.15, "unit": "g"}], "prepTime": "PT25M", "promotion": null, "ratingsCount": 16, "seoDescription": null, "seoName": null, "servingSize": 504, "slug": "aubergine-saag", "steps": [{"index": 1, "instructions": "\nPour 300ml cold salted water (double for 4p) into a medium pot with a tight-fitting lid.\nStir in the rice and bring to the boil.\nOnce boiling, lower the heat to medium and cover with the lid.\nCook for 10 mins then remove from the heat and leave aside for another 10 mins or until ready to serve (keep covered so the rice can continue to cook in its own steam).\n", "instructionsHTML": "<ul>\n<li>Pour 300ml cold <strong>salted water</strong> (double for 4p) into a medium pot with a tight-fitting lid.</li>\n<li>Stir in the <strong>rice </strong>and bring to the boil.</li>\n<li>Once boiling, lower the heat to medium and cover with the lid.</li>\n<li>Cook for 10 mins then remove from the heat and leave aside for another 10 mins or until ready to serve (keep covered so the rice can continue to cook in its own steam).</li>\n</ul>", "instructionsMarkdown": "<ul>\n<li>Pour 300ml cold <strong>salted water</strong> (double for 4p) into a medium pot with a tight-fitting lid.</li>\n<li>Stir in the <strong>rice </strong>and bring to the boil.</li>\n<li>Once boiling, lower the heat to medium and cover with the lid.</li>\n<li>Cook for 10 mins then remove from the heat and leave aside for another 10 mins or until ready to serve (keep covered so the rice can continue to cook in its own steam).</li>\n</ul>", "ingredients": [], "utensils": ["57e2674fb7e8697d4b3053a0", "57e2674fb7e8697d4b30538b", "57e2674fb7e8697d4b30537c", "57e2674fb7e8697d4b30536b"], "timers": [], "images": [{"link": "https://d3hvwccx09j84u.cloudfront.net/0,0/64074060683721c9a32b0936/step-29b6e200.jpg", "path": "/64074060683721c9a32b0936/step-29b6e200.jpg", "caption": "Make the Rice\t"}], "videos": []}, {"index": 2, "instructions": "\nHalve, peel and chop the onion into small pieces.\nQuarter the mushrooms. Chop any larger pieces so all are evenly sized.\nTrim the aubergine then chop into roughly 1cm pieces.\u00a0\nThinly slice the chilli at an angle widthways.\nChop the lime into 4 wedges.\n", "instructionsHTML": "<ul>\n<li>Halve, peel and chop the <strong>onion </strong>into small pieces.</li>\n<li>Quarter the <strong>mushrooms</strong>. Chop any larger pieces so all are evenly sized.</li>\n<li>Trim the <strong>aubergine </strong>then chop into roughly 1cm pieces.\u00a0</li>\n<li>Thinly slice the <strong>chilli </strong>at an angle widthways.</li>\n<li>Chop the <strong>lime</strong> into 4 wedges.</li>\n</ul>", "instructionsMarkdown": "<ul>\n<li>Halve, peel and chop the <strong>onion </strong>into small pieces.</li>\n<li>Quarter the <strong>mushrooms</strong>. Chop any larger pieces so all are evenly sized.</li>\n<li>Trim the <strong>aubergine </strong>then chop into roughly 1cm pieces.\u00a0</li>\n<li>Thinly slice the <strong>chilli </strong>at an angle widthways.</li>\n<li>Chop the <strong>lime</strong> into 4 wedges.</li>\n</ul>", "ingredients": [], "utensils": [], "timers": [], "images": [{"link": "https://d3hvwccx09j84u.cloudfront.net/0,0/64074060683721c9a32b0936/step-d994fe10.jpg", "path": "/64074060683721c9a32b0936/step-d994fe10.jpg", "caption": "Prep the Veg\t"}], "videos": []}, {"index": 3, "instructions": "\nPlace a large pan over high heat with a drizzle of oil.\nAdd the onion, mushroom and aubergine to the pan and fry until softened, stirring occasionally, 5-8 mins.\nSeason well with salt and pepper.\nLower the heat to medium-high then add the North Indian spice mix and fry for 1-2 mins.\n", "instructionsHTML": "<ul>\n<li>Place a large pan over high heat with a drizzle of <strong>oil</strong>.</li>\n<li>Add the <strong>onion</strong>, <strong>mushroom </strong>and <strong>aubergine </strong>to the pan and fry until softened, stirring occasionally, 5-8 mins.</li>\n<li>Season well with <strong>salt </strong>and <strong>pepper</strong>.</li>\n<li>Lower the heat to medium-high then add the <strong>North Indian spice mix</strong> and fry for 1-2 mins.</li>\n</ul>", "instructionsMarkdown": "<ul>\n<li>Place a large pan over high heat with a drizzle of <strong>oil</strong>.</li>\n<li>Add the <strong>onion</strong>, <strong>mushroom </strong>and <strong>aubergine </strong>to the pan and fry until softened, stirring occasionally, 5-8 mins.</li>\n<li>Season well with <strong>salt </strong>and <strong>pepper</strong>.</li>\n<li>Lower the heat to medium-high then add the <strong>North Indian spice mix</strong> and fry for 1-2 mins.</li>\n</ul>", "ingredients": [], "utensils": [], "timers": [], "images": [{"link": "https://d3hvwccx09j84u.cloudfront.net/0,0/64074060683721c9a32b0936/step-f81d3720.jpg", "path": "/64074060683721c9a32b0936/step-f81d3720.jpg", "caption": "Start the Sauce"}], "videos": []}, {"index": 4, "instructions": "\nMeanwhile, blend the spinach and coconut milk to make a puree.\nAdd the spinach puree to the pan and mix well to coat the veg.\u00a0\nCover and simmer for 8-10 mins. Loosen with a splash of water if necessary\nSeason with salt, pepper and lime juice\u2014all to taste!\n\nTIP: If you don't have a blender, finely chop the spinach instead.", "instructionsHTML": "<ul>\n<li>Meanwhile, blend the <strong>spinach </strong>and <strong>coconut milk</strong> to make a puree.</li>\n<li>Add the <strong>spinach </strong>puree to the pan and mix well to coat the veg.\u00a0</li>\n<li>Cover and simmer for 8-10 mins. Loosen with a splash of <strong>water </strong>if necessary</li>\n<li>Season with <strong>salt</strong>, <strong>pepper </strong>and <strong>lime </strong>juice\u2014all to taste!</li>\n</ul>\n<p>TIP: If you don&#39;t have a blender, finely chop the spinach instead.</p>", "instructionsMarkdown": "<ul>\n<li>Meanwhile, blend the <strong>spinach </strong>and <strong>coconut milk</strong> to make a puree.</li>\n<li>Add the <strong>spinach </strong>puree to the pan and mix well to coat the veg.\u00a0</li>\n<li>Cover and simmer for 8-10 mins. Loosen with a splash of <strong>water </strong>if necessary</li>\n<li>Season with <strong>salt</strong>, <strong>pepper </strong>and <strong>lime </strong>juice\u2014all to taste!</li>\n</ul>\n<p>TIP: If you don&#39;t have a blender, finely chop the spinach instead.</p>", "ingredients": [], "utensils": [], "timers": [], "images": [{"link": "https://d3hvwccx09j84u.cloudfront.net/0,0/64074060683721c9a32b0936/step-66d58830.jpg", "path": "/64074060683721c9a32b0936/step-66d58830.jpg", "caption": "Simmer the Saag"}], "videos": []}, {"index": 5, "instructions": "\nRoughly chop the coriander (stalks and all).\nStir the coriander through the cooked rice.\n", "instructionsHTML": "<ul>\n<li>Roughly chop the <strong>coriander </strong>(stalks and all).</li>\n<li>Stir the <strong>coriander</strong> through the cooked <strong>rice</strong>.</li>\n</ul>", "instructionsMarkdown": "<ul>\n<li>Roughly chop the <strong>coriander </strong>(stalks and all).</li>\n<li>Stir the <strong>coriander</strong> through the cooked <strong>rice</strong>